# Wololo Converter Backend Configuration

# Server settings
DEBUG=false
HOST=0.0.0.0
PORT=47652

# File storage
TEMP_DIR=/tmp/wololo
MAX_FILE_AGE_HOURS=1
CLEANUP_INTERVAL_MINUTES=15

# Download settings
MAX_CONCURRENT_DOWNLOADS=3
DOWNLOAD_TIMEOUT_SECONDS=3600
MAX_FILE_SIZE_MB=10000

# Rate limiting
RATE_LIMIT_DOWNLOADS=10/minute

# Database
DATABASE_URL=sqlite+aiosqlite:///./wololo.db

# CORS origins (comma-separated, for development)
CORS_ORIGINS=http://localhost:3000,http://localhost:47651
